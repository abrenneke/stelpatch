cb_claim {
    show_in_diplomacy = yes
    show_notification = no
    is_valid = {
        NOT = {
            is_overlord_to = FROM
        }

        has_total_war_cb = no
        OR = {
            has_claim = FROM
            from = {
                any_subject = {
                    root = {
                        has_claim = prev
                    }

                }

            }

        }

        OR = {
            overlord = {
                NOT = {
                    is_same_value = from
                }

            }

            is_subject = no
        }

    }

    potential = {
        NOT = {
            is_in_federation_with = from
        }

        is_country_type = default
    }

}

cb_subjugation {
    show_notification = no
    is_valid = {
        OR = {
            is_country_type = awakened_fallen_empire
            relative_power = {
                value > equivalent
                category = all
                who = FROM
            }

        }

        NOR = {
            has_crisis_level = crisis_level_1
            is_overlord_to = FROM
        }

        from = {
            is_subject = no
        }

    }

    destroy_if = {
        relative_power = {
            who = FROM
            category = all
            value < superior
        }

        NOT = {
            is_country_type = awakened_fallen_empire
        }

        has_crisis_level = crisis_level_1
    }

    potential = {
        NOT = {
            is_in_federation_with = from
        }

        is_subject = no
        OR = {
            is_country_type = default
            is_country_type = awakened_fallen_empire
        }

    }

    show_in_diplomacy = no
}

